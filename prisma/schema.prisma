generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters", "multiSchema"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["auth", "public"]
}

model GiftCardRun {
  id                String    @id @default(uuid())
  correlation_id    String    @unique
  square_event_id   String?
  square_event_type String?
  trigger_type      String
  resource_id       String?
  stage             String?
  status            String    @default("pending")
  attempts          Int       @default(0)
  last_error        String?
  payload           Json?
  context           Json?
  resumed_at        DateTime? @db.Timestamptz(6)
  created_at        DateTime  @default(now()) @db.Timestamptz(6)
  updated_at        DateTime  @default(now()) @db.Timestamptz(6)

  @@index([square_event_id])
  @@index([status])
  @@map("giftcard_runs")
  @@schema("public")
}

model GiftCardJob {
  id             String            @id @default(uuid())
  correlation_id String
  trigger_type   String
  stage          String
  status         GiftCardJobStatus @default(queued)
  payload        Json
  context        Json?
  attempts       Int               @default(0)
  max_attempts   Int               @default(5)
  scheduled_at   DateTime          @default(now()) @db.Timestamptz(6)
  locked_at      DateTime?         @db.Timestamptz(6)
  lock_owner     String?
  last_error     String?
  created_at     DateTime          @default(now()) @db.Timestamptz(6)
  updated_at     DateTime          @default(now()) @db.Timestamptz(6)

  @@unique([correlation_id, stage], map: "giftcard_jobs_correlation_stage_key")
  @@index([status, scheduled_at], map: "giftcard_jobs_status_scheduled_idx")
  @@map("giftcard_jobs")
  @@schema("public")
}

model WebhookJob {
  id               String           @id @default(uuid())
  event_type       String
  event_id         String
  event_created_at DateTime?        @db.Timestamptz(6)
  status           WebhookJobStatus @default(queued)
  payload          Json
  attempts         Int              @default(0)
  max_attempts     Int              @default(5)
  scheduled_at     DateTime         @default(now()) @db.Timestamptz(6)
  locked_at        DateTime?        @db.Timestamptz(6)
  lock_owner       String?
  last_error       String?
  created_at       DateTime         @default(now()) @db.Timestamptz(6)
  updated_at       DateTime         @default(now()) @db.Timestamptz(6)
  organization_id  String           @db.Uuid
  organization     Organization     @relation(fields: [organization_id], references: [id], onDelete: Cascade)

  @@unique([organization_id, event_id, event_type], map: "webhook_jobs_org_event_unique")
  @@index([organization_id])
  @@index([status, scheduled_at], map: "webhook_jobs_status_scheduled_idx")
  @@map("webhook_jobs")
  @@schema("public")
}

model ApplicationLog {
  id              String               @id @default(uuid())
  organization_id String?              @db.Uuid
  log_type        String
  log_id          String
  log_created_at  DateTime?            @db.Timestamptz(6)
  status          ApplicationLogStatus @default(received)
  payload         Json
  attempts        Int                  @default(0)
  max_attempts    Int                  @default(0)
  scheduled_at    DateTime             @default(now()) @db.Timestamptz(6)
  locked_at       DateTime?            @db.Timestamptz(6)
  lock_owner      String?
  last_error      String?
  created_at      DateTime             @default(now()) @db.Timestamptz(6)
  updated_at      DateTime             @default(now()) @db.Timestamptz(6)
  organization    Organization?        @relation(fields: [organization_id], references: [id])

  @@unique([organization_id, log_id, log_type], map: "application_logs_org_log_unique")
  @@index([organization_id])
  @@index([log_type])
  @@index([status, scheduled_at], map: "application_logs_status_scheduled_idx")
  @@index([created_at])
  @@map("application_logs")
  @@schema("public")
}

model NotificationEvent {
  id                 String                   @id @default(uuid())
  channel            NotificationChannel
  templateType       NotificationTemplateType
  status             NotificationStatus       @default(queued)
  customerId         String?
  referrerCustomerId String?
  referralEventId    String?
  externalId         String?                  @db.VarChar(255)
  templateId         String?                  @db.VarChar(255)
  sentAt             DateTime?
  statusAt           DateTime?
  errorCode          String?                  @db.VarChar(255)
  errorMessage       String?
  metadata           Json?
  createdAt          DateTime                 @default(now())
  organization_id    String                   @db.Uuid

  @@index([channel, status, createdAt], map: "notification_channel_status_idx")
  @@map("notification_events")
  @@schema("public")
}

model DevicePassRegistration {
  deviceLibraryIdentifier String
  passTypeIdentifier      String
  serialNumber            String
  pushToken               String?
  createdAt               DateTime @default(now())
  updatedAt               DateTime @updatedAt
  giftCardGan             String?
  customerName            String?
  customerEmail           String?
  squareCustomerId        String?
  giftCardId              String?
  balanceCents            Int?

  @@id([deviceLibraryIdentifier, passTypeIdentifier, serialNumber])
  @@map("device_pass_registrations")
  @@schema("public")
}

model SquareExistingClient {
  square_customer_id         String
  given_name                 String?
  family_name                String?
  email_address              String?
  phone_number               String?
  got_signup_bonus           Boolean?     @default(false)
  activated_as_referrer      Boolean?     @default(false)
  personal_code              String?
  created_at                 DateTime?    @default(now()) @db.Timestamptz(6)
  updated_at                 DateTime?    @default(now()) @db.Timestamptz(6)
  gift_card_id               String?
  referral_code              String?
  referral_url               String?
  total_referrals            Int?         @default(0)
  total_rewards              Int?         @default(0)
  email_sent_at              DateTime?    @db.Timestamptz(6)
  first_payment_completed    Boolean?     @default(false)
  ip_addresses               String[]     @default([])
  first_ip_address           String?
  last_ip_address            String?
  used_referral_code         String?
  referral_email_sent        Boolean?     @default(false)
  gift_card_order_id         String?
  gift_card_line_item_uid    String?
  gift_card_delivery_channel String?
  gift_card_activation_url   String?
  gift_card_pass_kit_url     String?
  gift_card_digital_email    String?
  referral_sms_sent          Boolean?     @default(false)
  referral_sms_sent_at       DateTime?    @db.Timestamptz(6)
  referral_sms_sid           String?
  gift_card_gan              String?
  organization_id            String       @db.Uuid
  id                         String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  raw_json                   Json?
  organization               Organization @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([organization_id, square_customer_id], map: "square_existing_clients_org_customer_idx")
  @@unique([organization_id, square_customer_id], map: "square_existing_clients_organization_square_customer_unique")
  @@index([activated_as_referrer], map: "idx_activated_referrer")
  @@index([gift_card_id], map: "idx_gift_card_id")
  @@index([personal_code], map: "idx_personal_code")
  @@index([referral_code], map: "idx_referral_code")
  @@index([gift_card_delivery_channel], map: "idx_square_clients_gift_card_delivery")
  @@index([gift_card_order_id], map: "idx_square_clients_gift_card_order")
  @@index([square_customer_id], map: "idx_square_customer_id")
  @@index([organization_id], map: "idx_square_existing_clients_organization_id")
  @@map("square_existing_clients")
  @@schema("public")
}

model CustomerAnalytics {
  organization_id                String       @db.Uuid
  square_customer_id             String
  given_name                     String?
  family_name                    String?
  email_address                  String?
  phone_number                   String?
  first_booking_at               DateTime?    @db.Timestamptz(3)
  last_booking_at                DateTime?    @db.Timestamptz(3)
  last_payment_at                DateTime?    @db.Timestamptz(3)
  total_accepted_bookings        Int          @default(0)
  total_cancelled_by_customer    Int          @default(0)
  total_cancelled_by_seller      Int          @default(0)
  total_no_shows                 Int          @default(0)
  total_revenue_cents            BigInt       @default(0)
  total_tips_cents               BigInt       @default(0)
  total_payments                 Int          @default(0)
  avg_ticket_cents               BigInt       @default(0)
  booking_notes                  Json?
  preferred_technician_id        String?
  preferred_service_variation_id String?
  distinct_locations             Int          @default(0)
  is_referrer                    Boolean      @default(false)
  activated_as_referrer_at       DateTime?    @db.Timestamptz(3)
  referral_source                String?
  total_referrals                Int          @default(0)
  total_rewards_cents            BigInt       @default(0)
  customer_segment               String       @default("NEW")
  created_at                     DateTime     @default(now()) @db.Timestamptz(3)
  updated_at                     DateTime     @default(now()) @updatedAt @db.Timestamptz(3)
  organization                   Organization @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([organization_id, square_customer_id])
  @@index([organization_id, customer_segment])
  @@index([organization_id, first_booking_at])
  @@index([organization_id, last_booking_at])
  @@index([is_referrer])
  @@index([booking_notes], map: "customer_analytics_booking_notes_gin_idx", type: Gin)
  @@map("customer_analytics")
  @@schema("public")
}

model square_gift_card_gan_audit {
  gift_card_id       String    @id
  square_customer_id String?
  resolved_gan       String?
  verified_at        DateTime? @default(now()) @db.Timestamptz(6)
  raw_payload        Json?

  @@schema("public")
}

model GiftCard {
  id                      String                @id @default(uuid())
  square_customer_id      String
  square_gift_card_id     String
  gift_card_gan           String?
  reward_type             GiftCardRewardType
  initial_amount_cents    Int?
  current_balance_cents   Int?
  gift_card_order_id      String?
  gift_card_line_item_uid String?
  delivery_channel        String?
  activation_url          String?
  pass_kit_url            String?
  digital_email           String?
  is_active               Boolean               @default(true)
  state                   String?
  created_at              DateTime              @default(now()) @db.Timestamptz(6)
  updated_at              DateTime              @updatedAt @db.Timestamptz(6)
  last_balance_check_at   DateTime?             @db.Timestamptz(6)
  organization_id         String                @db.Uuid
  transactions            GiftCardTransaction[]
  organization            Organization          @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  ReferralReward          ReferralReward[]

  @@unique([organization_id, square_gift_card_id])
  @@index([square_customer_id])
  @@index([square_gift_card_id])
  @@index([gift_card_gan])
  @@index([reward_type])
  @@index([is_active])
  @@index([organization_id], map: "idx_gift_cards_organization_id")
  @@map("gift_cards")
  @@schema("public")
}

model GiftCardTransaction {
  id                   String                  @id @default(uuid())
  gift_card_id         String
  transaction_type     GiftCardTransactionType
  amount_cents         Int
  balance_before_cents Int?
  balance_after_cents  Int?
  square_activity_id   String?
  square_order_id      String?
  square_payment_id    String?
  reason               String?
  context_label        String?
  metadata             Json?
  created_at           DateTime                @default(now()) @db.Timestamptz(6)
  organization_id      String?                 @db.Uuid
  giftCard             GiftCard                @relation(fields: [gift_card_id], references: [id])

  @@index([gift_card_id])
  @@index([transaction_type])
  @@index([created_at])
  @@index([square_activity_id])
  @@map("gift_card_transactions")
  @@schema("public")
}

model ReferralProfile {
  id                    String           @id @default(uuid())
  square_customer_id    String           @unique
  personal_code         String?          @unique
  referral_code         String?          @unique
  referral_url          String?
  activated_as_referrer Boolean          @default(false)
  used_referral_code    String?
  referral_email_sent   Boolean          @default(false)
  referral_sms_sent     Boolean          @default(false)
  referral_sms_sent_at  DateTime?        @db.Timestamptz(6)
  referral_sms_sid      String?
  total_referrals_count Int              @default(0)
  total_rewards_cents   Int              @default(0)
  created_at            DateTime         @default(now()) @db.Timestamptz(6)
  updated_at            DateTime         @updatedAt @db.Timestamptz(6)
  activated_at          DateTime?        @db.Timestamptz(6)
  organization_id       String           @db.Uuid
  referral_checked_at   DateTime?        @db.Timestamptz(6)
  organization          Organization     @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  rewards               ReferralReward[]

  @@unique([organization_id, square_customer_id])
  @@index([square_customer_id])
  @@index([personal_code])
  @@index([referral_code])
  @@index([used_referral_code])
  @@index([activated_as_referrer])
  @@index([organization_id], map: "idx_referral_profiles_organization_id")
  @@map("referral_profiles")
  @@schema("public")
}

model ReferralReward {
  id                   String               @id @default(uuid())
  referrer_customer_id String
  referred_customer_id String
  reward_amount_cents  Int
  status               ReferralRewardStatus @default(PAID)
  gift_card_id         String?
  payment_id           String?
  booking_id           String?
  reward_type          String?
  metadata             Json?
  created_at           DateTime             @default(now()) @db.Timestamptz(6)
  paid_at              DateTime?            @db.Timestamptz(6)
  organization_id      String               @db.Uuid
  giftCard             GiftCard?            @relation(fields: [gift_card_id], references: [id])
  organization         Organization         @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  referrer             ReferralProfile      @relation(fields: [referrer_customer_id], references: [square_customer_id])

  @@index([referrer_customer_id])
  @@index([referred_customer_id])
  @@index([gift_card_id])
  @@index([payment_id])
  @@index([booking_id])
  @@index([status])
  @@index([created_at])
  @@index([organization_id], map: "idx_referral_rewards_organization_id")
  @@map("referral_rewards")
  @@schema("public")
}

model Organization {
  id                      String                 @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  square_merchant_id      String                 @unique
  created_at              DateTime               @default(now()) @db.Timestamptz(6)
  updated_at              DateTime               @default(now()) @updatedAt @db.Timestamptz(6)
  name                    String?
  slug                    String?
  settings                Json?                  @default("{}")
  is_active               Boolean?               @default(true)
  application_logs        ApplicationLog[]
  bookings                Booking[]
  customer_analytics      CustomerAnalytics[]
  gift_cards              GiftCard[]
  locations               Location[]
  order_line_items        OrderLineItem[]
  orders                  Order[]
  organization_users      OrganizationUser[]
  payment_tenders         PaymentTender[]
  payments                Payment[]
  referral_profiles       ReferralProfile[]
  referral_rewards        ReferralReward[]
  service_variations      ServiceVariation[]
  square_existing_clients SquareExistingClient[]
  team_members            TeamMember[]
  user_roles              UserRole[]
  webhook_jobs            WebhookJob[]
  admin_analytics_daily   AdminAnalyticsDaily[]

  @@index([square_merchant_id], map: "idx_organizations_square_merchant_id")
  @@map("organizations")
  @@schema("public")
}

model AdminAnalyticsDaily {
  // === COMPOSITE KEY ===
  organization_id     String   @db.Uuid
  team_member_id      String   @db.Uuid             // internal UUID from team_members.id
  location_id         String   @db.Uuid             // FK to locations.id
  date_pacific        DateTime @db.Date             // DATE(start_at AT TIME ZONE Pacific)
  timezone            String   @default("America/Los_Angeles")

  // === ADMIN INFO (denormalized for fast reads) ===
  given_name          String?
  family_name         String?
  role                String?                       // from TeamMember.role

  // === ACTIVITY METRICS (by created_at) ===
  bookings_created_count              Int @default(0)   // only creator_type='TEAM_MEMBER'
  new_customers_booked_count          Int @default(0)   // new customers booked by admin today

  // === VISITS (Creator-based, by start_at) ===
  appointments_total                  Int @default(0)   // all appointments on this day
  appointments_accepted               Int @default(0)
  appointments_no_show                Int @default(0)
  appointments_cancelled              Int @default(0)   // total cancelled
  late_cancellations                  Int @default(0)   // cancelled < 24h before start_at

  // === CREATOR MONEY (Attributed to booking creator) ===
  creator_payments_count              Int    @default(0)
  creator_revenue_cents               BigInt @default(0)
  creator_avg_ticket_cents            BigInt @default(0)

  // === CASHIER MONEY (Attributed to payment administrator) ===
  cashier_payments_count              Int    @default(0)
  cashier_revenue_cents               BigInt @default(0)
  cashier_tips_cents                  BigInt @default(0)
  cashier_avg_ticket_cents            BigInt @default(0)

  // === AUDIT ===
  created_at           DateTime @default(now()) @db.Timestamptz(6)
  updated_at           DateTime @updatedAt @db.Timestamptz(6)

  // === RELATIONS ===
  organization  Organization @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  team_member   TeamMember   @relation(fields: [team_member_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  location      Location     @relation(fields: [location_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  // === CONSTRAINTS ===
  @@id([organization_id, team_member_id, location_id, date_pacific])
  @@index([organization_id, date_pacific])
  @@index([organization_id, team_member_id])
  @@index([organization_id, location_id, date_pacific])
  @@map("admin_analytics_daily")
  @@schema("public")
}

model Location {
  id                              String       @id @default(uuid()) @db.Uuid
  square_location_id              String
  name                            String
  address_line_1                  String?
  locality                        String?
  administrative_district_level_1 String?
  postal_code                     String?
  created_at                      DateTime     @default(now())
  updated_at                      DateTime     @updatedAt
  organization_id                 String       @db.Uuid
  square_merchant_id              String?      @db.VarChar(255)
  bookings                        Booking[]
  organization                    Organization @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  orders                          Order[]
  payments                        Payment[]
  user_roles                      UserRole[]
  admin_analytics_daily           AdminAnalyticsDaily[]

  @@unique([organization_id, square_location_id], map: "locations_organization_square_location_unique")
  @@index([organization_id], map: "idx_locations_organization_id")
  @@index([square_location_id], map: "idx_locations_square_location_id")
  @@index([square_merchant_id], map: "idx_locations_square_merchant_id")
  @@map("locations")
  @@schema("public")
}

model TeamMember {
  id                                String           @id @default(uuid()) @db.Uuid
  square_team_member_id             String
  given_name                        String?
  family_name                       String?
  email_address                     String?
  phone_number                      String?
  status                            String?
  location_code                     TeamLocation     @default(UNKNOWN_LOC)
  role                              TeamRole         @default(UNKNOWN)
  is_system                         Boolean          @default(false)
  created_at                        DateTime         @default(now())
  updated_at                        DateTime         @updatedAt
  organization_id                   String           @db.Uuid
  booking_segments_as_technician    BookingSegment[] @relation("BookingSegmentTechnician")
  bookings_as_administrator         Booking[]        @relation("BookingAdministrator")
  bookings_as_technician            Booking[]        @relation("BookingTechnician")
  order_line_items_as_administrator OrderLineItem[]  @relation("OrderLineItemAdministrator")
  order_line_items_as_technician    OrderLineItem[]  @relation("OrderLineItemTechnician")
  payments_as_administrator         Payment[]        @relation("PaymentAdministrator")
  organization                      Organization     @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  admin_analytics_daily             AdminAnalyticsDaily[]

  @@unique([organization_id, square_team_member_id], map: "team_members_organization_square_team_member_unique")
  @@index([square_team_member_id])
  @@index([status])
  @@index([is_system])
  @@index([organization_id], map: "idx_team_members_organization_id")
  @@map("team_members")
  @@schema("public")
}

model ServiceVariation {
  uuid                String           @id @db.Uuid
  square_variation_id String           @unique
  name                String?
  duration_minutes    Int?
  created_at          DateTime         @default(now())
  updated_at          DateTime         @updatedAt
  organization_id     String           @db.Uuid
  item_id             String?
  service_duration    Int?
  team_member_ids     String[]         @default([])
  service_name        String?
  bookingSegments     BookingSegment[]
  bookings            Booking[]
  organization        Organization     @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([organization_id, square_variation_id], map: "service_variation_organization_square_variation_id_unique")
  @@index([organization_id], map: "idx_service_variation_organization_id")
  @@index([item_id])
  @@map("service_variation")
  @@schema("public")
}

model Booking {
  customer_id                     String?
  location_type                   String?
  start_at                        DateTime
  status                          String
  all_day                         Boolean           @default(false)
  version                         Int               @default(0)
  source                          String?
  address_line_1                  String?
  locality                        String?
  administrative_district_level_1 String?
  postal_code                     String?
  creator_type                    String?
  creator_customer_id             String?
  transition_time_minutes         Int?              @default(0)
  created_at                      DateTime
  updated_at                      DateTime
  raw_json                        Json?
  merchant_id                     String?
  service_variation_version       BigInt?
  duration_minutes                Int?
  intermission_minutes            Int?              @default(0)
  any_team_member                 Boolean?          @default(false)
  booking_id                      String
  id                              String            @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  organization_id                 String            @db.Uuid
  location_id                     String            @db.Uuid
  service_variation_id            String?           @db.Uuid
  technician_id                   String?           @db.Uuid
  administrator_id                String?           @db.Uuid
  customer_note                   String?
  seller_note                     String?
  segments                        BookingSegment[]
  administrator                   TeamMember?       @relation("BookingAdministrator", fields: [administrator_id], references: [id], onDelete: Restrict, onUpdate: NoAction)
  location                        Location          @relation(fields: [location_id], references: [id], onUpdate: NoAction)
  organization                    Organization      @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  service_variation               ServiceVariation? @relation(fields: [service_variation_id], references: [uuid], onDelete: Restrict, onUpdate: NoAction)
  technician                      TeamMember?       @relation("BookingTechnician", fields: [technician_id], references: [id], onDelete: Restrict, onUpdate: NoAction)
  order_line_items                OrderLineItem[]
  orders                          Order[]

  @@unique([organization_id, booking_id], map: "bookings_organization_booking_id_unique")
  @@index([customer_id])
  @@index([start_at])
  @@index([status])
  @@index([created_at])
  @@index([creator_type])
  @@index([booking_id], map: "idx_bookings_booking_id")
  @@index([organization_id], map: "idx_bookings_organization_id")
  @@map("bookings")
  @@schema("public")
}

model BookingSegment {
  id                          String            @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  booking_id                  String            @db.Uuid
  segment_index               Int
  square_segment_uid          String?
  square_service_variation_id String?
  service_variation_id        String?           @db.Uuid
  service_variation_version   BigInt?
  duration_minutes            Int?
  intermission_minutes        Int?              @default(0)
