  @@index([organization_id])
  @@index([location_id])
  @@index([customer_id])
  @@index([booking_id])
  @@index([order_id])
  @@index([state])
  @@index([technician_id])
  @@index([administrator_id])
  @@map("orders")
  @@schema("public")
}

